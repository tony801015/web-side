{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "prestart": "npm i && sh ./script/redis.sh",
    "start": "node app.js",
    "test": "npm i && jest ./tests/lib/* --coverage",
    "e2e": "npm run prestart && jest --forceExit --coverage",
    "dev": "nodemon app.js"
  },
  "keywords": [
    "rate-limit"
  ],
  "author": "Tony",
  "license": "ISC",
  "dependencies": {
    "express": "4.17.3",
    "ioredis": "4.14.1"
  },
  "devDependencies": {
    "module-alias": "2.2.2",
    "nodemon": "1.19.3",
    "jest": "24.9.0",
    "supertest": "4.0.2"
  },
  "_moduleAliases": {
    "@root": ".",
    "@lib": "./lib",
    "@config": "./config",
    "@controller": "./controller",
    "@middleware": "./middleware"
  },
  "jest": {
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/$1",
      "@lib/(.*)": "<rootDir>/lib/$1",
      "@config/(.*)": "<rootDir>/config/$1",
      "@controller/(.*)": "<rootDir>/controller/$1",
      "@middleware/(.*)": "<rootDir>/middleware/$1"
    }
  }
}
